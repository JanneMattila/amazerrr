<Window x:Class="AmazerrApp.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:AmazerrApp"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <TextBox x:Name="BackendAddress" HorizontalAlignment="Left" Margin="10,10,0,0" Text="http://localhost:7071" TextWrapping="Wrap" VerticalAlignment="Top" Width="600" FontSize="24" />
        <Button x:Name="SolveButton" Content="Solve" HorizontalAlignment="Left" Margin="10,55,0,0" VerticalAlignment="Top" FontSize="24" Click="SolveButton_Click" Width="100" />
        <TextBox x:Name="Swipes" HorizontalAlignment="Left" Margin="10,111,0,0" Text="" TextWrapping="Wrap" VerticalAlignment="Top" Width="600"  FontSize="24" Height="313" />
    </Grid>
</Window>
